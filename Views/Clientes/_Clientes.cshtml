@{ List<ControleFinanc.Models.Clientes> md = ViewBag.Clientes;}

<div class="container">
    <div class="row">
        <!--Alterar id em partial novos-->
        <div id="partialClientes">
            <table class="table table-condensed table-hover">
                <tr>
                    <td><strong>ID</strong></td>
                    <td><strong>NOME</strong></td>
                    <td><strong>TELEFONE</strong></td>
                    <td><strong>CIDADE</strong></td>
                    <td><img src="~/Content/Img/edit_pie_chart_report_30px.png" sizes="30px" /></td>
                    <td><img src="~/Content/Img/delete_30px.png" sizes="30px" /></td>
                </tr>

                @foreach (var Cli in md)
                {
                    <tr>
                        <td>@Cli.Id</td>
                        <td>@Cli.Nome</td>
                        <td>@Cli.Telefone_1</td>
                        <td>@Cli.Cidade</td>
                        <td><a href="/Clientes/Editar/@Cli.Id"><strong>Editar</strong></a></td>
                        <td><a href="/Clientes/Excluir/@Cli.Id" class="text-danger"><strong>Excluir</strong></a></td>
                    </tr>
                }

            </table>
        </div>
    </div>
</div>
